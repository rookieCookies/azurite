//test numbers
assert_info(1 < 2,  "1 < 2")
assert_info(1 == 1, "1 == 1")
assert_info(2 > 1,  "2 > 1")
assert_info(1 <= 2, "1 <= 2")
assert_info(2 >= 1, "2 >= 1")


//test variables
var a = 1
var b = 2

assert_info(a < b,  "a < b")
assert_info(a == a, "a == a")
assert_info(b > a,  "b > a")
assert_info(a <= b, "a <= b")
assert_info(b >= a, "b >= a")


//test negation
assert_info(!false, "!false")

var c = false
assert_info(!c, "!c")